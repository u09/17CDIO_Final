<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" prefHeight="199.0" prefWidth="239.0" spacing="20.0" stylesheets="@../../QuickConnectCSS/StandardLayout.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lCode" text="Indtast kode fra mail:" />
      <TextField fx:id="inCode" minWidth="-Infinity" prefHeight="27.0" prefWidth="159.0" promptText="Kode">
         <VBox.margin>
            <Insets left="30.0" right="30.0" />
         </VBox.margin>
      </TextField>
      <HBox alignment="CENTER" prefHeight="21.0" prefWidth="239.0" spacing="25.0">
         <children>
            <Button fx:id="bAccept" mnemonicParsing="false" text="Godkend" />
            <Button fx:id="bSend" mnemonicParsing="false" text="Send Igen" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
